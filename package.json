{
  "name": "shopflow-api",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devstart": "npm run dbmigrate && PORT=8080 NODE_ENV=dev node ./bin/www",
    "stagingstart": "npm run dbmigrate && PORT=8080 NODE_ENV=staging node ./bin/www",
    "prodstart": "npm run dbmigrate && PORT=8080 NODE_ENV=prod node ./bin/www",
    "dbmigrate": "DATABASE_URL=$MONGO_URL node node_modules/db-migrate/bin/db-migrate up"
  },
