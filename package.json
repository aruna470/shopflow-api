    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "db-migrate": "^0.10.7",
    "db-migrate-mongodb": "^1.4.0",
    "debug": "~2.6.9",
    "email-validator": "^1.1.1",
    "express": "~4.15.5",
    "express-validator": "^5.0.3",
    "generate-password": "^1.4.0",
    "hashmap": "^2.3.0",
    "http-proxy": "^1.16.2",
    "is-empty": "^1.2.0",
